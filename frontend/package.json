		"@emotion/react": "^11.7.1",
		"@emotion/styled": "^11.6.0",
		"@mui/icons-material": "^5.3.1",
		"@mui/material": "^5.4.0",
		"axios": "^0.25.0",
		"deep-equal": "^2.0.5",
		"react": "^17.0.2",
		"react-cookie": "^4.1.1",
		"react-dom": "^17.0.2",
		"react-router-dom": "^6.2.1",
		"react-scroll": "^1.8.6",
