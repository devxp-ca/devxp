		"@babel/preset-react": "^7.16.7",
		"@types/deep-equal": "^1.0.1",
		"@types/jest": "^27.4.0",
		"@types/node": "^17.0.12",
		"@types/react": "^17.0.38",
		"@types/react-dom": "^17.0.11",
		"@types/react-router-dom": "^5.3.3",
		"@types/react-scroll": "^1.8.3",
