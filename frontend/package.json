{
	"name": "dxaas-frontend",
	"version": "1.0.0",
	"description": "React frontend for DXaaS webapp",
	"main": "dist/index.js",
	"scripts": {
		"test": "echo 'Tests not setup' ; exit 1",
		"dev": "webpack-dev-server --config ./webpack.config.js",
		"dev:inject": "nodemon --watch 'dist/*' -e js,html --exec 'cp dist/* ../backend/public/'",
		"dev:watch": "webpack --watch --config ./webpack.config.js",
		"build": "webpack --config ./webpack.config.js --mode=production",
		"lint": "eslint .",
		"lint:fix": "eslint --fix",
		"format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" --config ./.prettierrc.json",
		"prepare": "cd .. && husky install frontend/.husky",
		"typedoc": "typedoc",
		"dev:full": "../dev.sh"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/emerald-river/startup.git"
	},
	"author": "Emerald River",
	"license": "ISC",
